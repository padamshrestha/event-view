{
  "name": "event-view",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"lite-server\" ",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc -p ./src",
    "tsc:w": "tsc -w -p ./src",
    "typings": "typings",
    "minify": "./node_modules/.bin/uglifyjs dist/temp.js --compress --mangle --screw-ie8 > dist/bundle.js",
    "clean": "rm -rf ./src/aot && rm -rf ./dist",
    "aot": "node_modules/.bin/ngc -p ./src",
    "rollup": "./node_modules/.bin/rollup -c ./rollup.js",
    "install-application": "npm run clean && npm install && npm run aot && npm run rollup && gulp es5 && npm run minify"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/platform-server": "2.0.0-rc.6",
    "@angular/upgrade": "2.0.0-rc.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "rxjs-es": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17",
    "angular2-in-memory-web-api": "0.0.18",
    "rollup-plugin-includepaths": "^0.1.5",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.7.4",
    "lodash": "^4.11.1",
    "jasmine-core": "~2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-jshint": "^2.0.1",
    "gulp-minify": "0.0.11",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "rollup": "^0.26.7",
    "rollup-plugin-node-resolve": "^2.0.0",
    "systemjs-builder": "^0.15.16",
    "traceur": "^0.0.91",
    "uglify-js": "2.7.0"
  }
}
